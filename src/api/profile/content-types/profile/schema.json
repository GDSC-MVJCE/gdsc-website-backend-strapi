        "files",
        "videos",
        "audios"
      ]
    },
    "coverPhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "blogs": {
      "type": "relation",
